return Math.max.apply(null,durations.map((d,i)=>{return toMs(d)+toMs(delays[i]);}));}
function toMs(s){return Number(s.slice(0,-1).replace(",","."))*1000;}
function whenTransitionEnd(elm,cb){if(!elm.parentNode){return;}
