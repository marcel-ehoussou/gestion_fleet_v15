return;}}}
if(node.nodeName!=="t"||node.hasAttribute("t-tag")){let nodeHooks={};let addNodeHook=function(hook,handler){nodeHooks[hook]=nodeHooks[hook]||[];nodeHooks[hook].push(handler);};if(node.tagName==="select"&&node.hasAttribute("t-att-value")){const value=node.getAttribute("t-att-value");let exprId=ctx.generateID();ctx.addLine(`let expr${exprId} = ${ctx.formatExpression(value)};`);let expr=`expr${exprId}`;node.setAttribute("t-att-value",expr);addNodeHook("create",`n.elm.value=${expr};`);}
