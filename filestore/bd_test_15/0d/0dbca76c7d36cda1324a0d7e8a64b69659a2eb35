return;cb();browser.clearTimeout(fallbackTimeout);elm.removeEventListener("transitionend",transitionEndCB);};elm.addEventListener("transitionend",transitionEndCB,{once:true});const fallbackTimeout=browser.setTimeout(transitionEndCB,timeout+1);}
else{cb();}}
