function getNextTangibleNode(node){while(node&&!isTangible(node)){node=node.nextSibling;}
return node;}
__exports.getDeepestPosition=getDeepestPosition;function getDeepestPosition(node,offset){let found=false;while(node.hasChildNodes()){let newNode=node.childNodes[offset];if(newNode){newNode=getNextTangibleNode(newNode);if(!newNode||isVisibleEmpty(newNode))break;found=true;node=newNode;offset=0;}else{break;}}
