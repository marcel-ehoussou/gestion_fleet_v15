/* /web/static/src/legacy/js/core/error_utils.js defined in bundle 'web.assets_common_lazy' */
odoo.define('@web/legacy/js/core/error_utils',async function(require){'use strict';let __exports={};const{loadJS}=require("web.ajax");const BrowserDetection=require("web.BrowserDetection");const{_t}=require("web.core");__exports.formatTraceback=formatTraceback;function formatTraceback(error){const traceback=error.stack;const browserDetection=new BrowserDetection();if(browserDetection.isBrowserChrome()){return error.stack;}else{return`${_t("Error:")} ${error.message}\n${error.stack}`;}}
