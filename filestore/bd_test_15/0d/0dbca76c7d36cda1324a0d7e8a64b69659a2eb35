if(tour.ready)return Promise.resolve();const tour_is_consumed=this._isTourConsumed(name);return tour.wait_for.then((function(){tour.current_step=parseInt(local_storage.getItem(get_step_key(name)))||0;tour.steps=_.filter(tour.steps,(function(step){return(!step.edition||step.edition===this.edition)&&(step.mobile===undefined||step.mobile===config.device.isMobile);}).bind(this));if(tour_is_consumed||tour.current_step>=tour.steps.length){local_storage.removeItem(get_step_key(name));tour.current_step=0;}
